{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"formatter":{
		"indentStyle":"space",
		"lineWidth": 100
	},
	"javascript": {
		"formatter": {
			"quoteStyle":"single",
      "trailingCommas": "es5"
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}
